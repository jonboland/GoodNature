@model int

<a asp-action="Edit" asp-route-id="@Model" class="mr-2 text-primary"><i class="fa-solid fa-pen"></i></a>
<a asp-action="Details" asp-route-id="@Model" class="mr-2 text-dark"><i class="fa-solid fa-circle-info"></i></a>
<a asp-action="Delete" asp-route-id="@Model" class="mr-2 text-danger"><i class="fa-solid fa-trash-can"></i></a>
